{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "types": ["vitest/globals"],
    "module": "esnext"
  },
  "exclude": ["node_modules", "use-cases"]
}
